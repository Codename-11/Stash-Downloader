{
  "name": "stash-plugins",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "Monorepo for Stash plugins",
  "workspaces": [
    "plugins/*",
    "shared"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "build:downloader": "npm run build -w @stash-plugins/stash-downloader",
    "build:browser": "npm run build -w @stash-plugins/stash-browser",
    "dev:downloader": "npm run dev -w @stash-plugins/stash-downloader",
    "dev:browser": "npm run dev -w @stash-plugins/stash-browser",
    "test": "npm run test --workspaces --if-present",
    "type-check": "npm run type-check --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present"
  },
  "keywords": [
    "stash",
    "plugins",
    "monorepo"
  ],
  "author": "",
  "license": "MIT"
}
